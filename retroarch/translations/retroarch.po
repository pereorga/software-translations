msgid "No Images Available"
msgstr "No hi ha imatges disponibles"

msgctxt "MENU_ENUM_LABEL_VALUE_NO_MUSIC_AVAILABLE"
msgid "No Music Available"
msgstr "No hi ha música disponible"

msgctxt "MENU_ENUM_LABEL_VALUE_NO_VIDEOS_AVAILABLE"
msgid "No Videos Available"
msgstr "No hi ha vídeos disponibles"

msgctxt "MENU_ENUM_LABEL_VALUE_NO_INFORMATION_AVAILABLE"
msgid "No Information Available"
