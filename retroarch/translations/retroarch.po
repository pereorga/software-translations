msgstr "Joc desconegut"

msgctxt "MENU_ENUM_LABEL_VALUE_CANNOT_ACTIVATE_ACHIEVEMENTS_WITH_THIS_CORE"
msgid "Achievements can't be activated with this core"
msgstr "No es pot activar els assoliments amb aquest nucli"

msgctxt "MENU_ENUM_LABEL_VALUE_CONTENT_INFO_CHEEVOS_HASH"
