msgctxt "MENU_ENUM_LABEL_VALUE_VIDEO_RECORD_THREADS"
msgid "Recording Threads"
msgstr "Fils per a l'enregistrament"

msgctxt "MENU_ENUM_LABEL_VALUE_VIDEO_POST_FILTER_RECORD"
msgid "Use Post Filter Recording"
msgstr "Grava amb els filtres aplicats"

msgctxt "MENU_ENUM_SUBLABEL_VIDEO_POST_FILTER_RECORD"
msgid "Capture the image after filters (but not shaders) are applied. The video will look as fancy as what you see on your screen."
msgstr "Captura la imatge després d'aplicar els filtres (però no els shaders). El vídeo es veurà tan elegant com el veieu a la pantalla."

msgctxt "MENU_ENUM_LABEL_VALUE_VIDEO_GPU_RECORD"
msgid "Use GPU Recording"
msgstr "Fes servir l'enregistrament per GPU"

msgctxt "MENU_ENUM_SUBLABEL_VIDEO_GPU_RECORD"
msgid "Record output of GPU shaded material if available."
msgstr "Enregistra la sortida de la GPU amb shaders, si està disponible."

msgctxt "MENU_ENUM_LABEL_VALUE_STREAMING_MODE"
msgid "Streaming Mode"
msgstr "Mode de retransmissió"

msgctxt "MENU_ENUM_LABEL_VALUE_VIDEO_STREAMING_MODE_CUSTOM"
