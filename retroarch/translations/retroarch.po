msgstr "Configuració personalitzada de retransmissió"

msgctxt "MENU_ENUM_LABEL_VALUE_STREAMING_TITLE"
msgid "Stream Title"
msgstr "Emetre títol"

msgctxt "MENU_ENUM_LABEL_VALUE_STREAMING_URL"
msgid "Stream URL"
msgstr "URL de transmissió"

msgctxt "MENU_ENUM_LABEL_VALUE_UDP_STREAM_PORT"
