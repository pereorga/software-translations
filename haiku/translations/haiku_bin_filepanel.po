#: FilePanelApp
msgid "display a load/save file panel\\n"
msgstr "mostra un quadre per carregar o desar un fitxer\\n"

#: FilePanelApp
msgid "short\\tlong\\tdescription\\n"
msgstr "descripció\\t curta\\tllarga\\n"

#: FilePanelApp
msgid "%s not a valid option\\n"
msgstr "%s no és una opció vàlida\\n"

#: FilePanelApp
msgid "-s\\t--save\\tuse a save FilePanel\\n"
msgstr "-s\\t--save\\tfusa un quadre de desament del FilePanel\\n"

#: FilePanelApp
msgid "%s: this option requires a parameter\\n"
msgstr "%s: amb aquesta opció cal un paràmetre\\n"

#: FilePanelApp
msgid "-h\\t--help\\tdisplay usage\\n"
msgstr "-h\\t--help\\tmostra'n l'ús\\n"

#: FilePanelApp
msgid "-l\\t--load\\tuse a load FilePanel (default)\\n"
msgstr "-l\\t--load\\tusa un quadre de càrrega del FilePanel (per defecte)\\n"

#: FilePanelApp
msgid "-n\\t--name\\tset the default name for saving\\n"
msgstr "-n\\t--name\\testableix el nom per defecte per desar-ho\\n"

#: FilePanelApp
msgid ""
"-k\\t--kind\\tkind of entries that can be opened (flavour): any combination "
"of f, d, s (file (default), directory, symlink)\\n"
msgstr ""
"-k\\t--kind\\ttipus d'entrades que es poden obrir (essència): qualsevol "
"combinació de f, d, s (fitxer (per defecte), carpeta, enllaç simbòlic)\\n"

#: FilePanelApp
msgid ""
"usage: %s [-h]     [-d folder]              [-l|-s]     [-t ttl]      [-1]"
"       [-m]"
msgstr ""
"ús: %s [-h]     [-d carpeta]              [-l|-s]     [-t ttl]      [-1]"
"       [-m]"

#: FilePanelApp
msgid ""
"usage: %s [--help] [--directory folder] [--load|--save] [--title ttl] "
"[--single] [--modal]\\n"
msgstr ""
"ús: %s [--help] [--directory folder] [--load|--save] [--title ttl] [--single]"
" [--modal]\\n"

#: FilePanelApp
msgid "-t\\t--title\\tset the FilePanel window title\\n"
msgstr "-t\\t--title\\tiestableix el títol de finestra del FilePanel\\n"

#: FilePanelApp
msgid "-1\\t--single\\tallow only 1 file to be selected\\n"
msgstr "-1\\t--single\\tpermet seleccionar 1 sol fitxer\\n"

#: FilePanelApp
msgid "-m\\t--modal\\tmakes the FilePanel modal\\n"
msgstr "-m\\t--modal\\tfa que el FilePanel sigui modal\\n"

#: FilePanelApp
msgid "options:\\n"
msgstr "opcions:\\n"

#: FilePanelApp
msgid "-d\\t--directory\\topen at <folder>\\n"
msgstr "-d\\t--directory\\tobre a <carpeta>\\n"
