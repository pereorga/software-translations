msgctxt "STR_CONFIG_SETTING_TYPE_HIDES"
msgid "Show all search results by setting{}{PUSH_COLOUR}{SILVER}Type {POP_COLOUR}to {WHITE}All setting types"
msgstr "Mostrar tots els resultats de la cerca filtrada amb {WHITE}tots els tipus de paràmetres"

msgctxt "STR_CONFIG_SETTING_CATEGORY_AND_TYPE_HIDES"
msgid "Show all search results by setting{}{PUSH_COLOUR}{SILVER}Category {POP_COLOUR}to {PUSH_COLOUR}{WHITE}{STRING} {POP_COLOUR}and {PUSH_COLOUR}{SILVER}Type {POP_COLOUR}to {WHITE}All setting types"
msgstr "Mostra tots els resultats de la cerca establint {SILVER}«Categoria» {BLACK}a {WHITE}«{STRING}» {BLACK}i {SILVER}«Tipus» {BLACK}a {WHITE}«Tots els paràmetres»{BLACK}."

msgctxt "STR_CONFIG_SETTING_OFF"
msgid "Off"
msgstr "Inactiu"

msgctxt "STR_CONFIG_SETTING_ON"
msgid "On"
msgstr "Actiu"

msgctxt "STR_CONFIG_SETTING_DISABLED"
msgid "Disabled"
msgstr "Desactivat"

msgctxt "STR_CONFIG_SETTING_COMPANIES_OFF"
msgid "Off"
msgstr "Inactiu"

msgctxt "STR_CONFIG_SETTING_COMPANIES_OWN"
msgid "Own company"
msgstr "Només de la companyia pròpia"

msgctxt "STR_CONFIG_SETTING_COMPANIES_ALL"
msgid "All companies"
msgstr "De totes les companyies"

msgctxt "STR_CONFIG_SETTING_NONE"
msgid "None"
msgstr "Cap"

msgctxt "STR_CONFIG_SETTING_REALISTIC"
msgid "Realistic"
msgstr "Realista"

msgctxt "STR_CONFIG_SETTING_HORIZONTAL_POS_LEFT"
msgid "Left"
msgstr "Esquerra"

msgctxt "STR_CONFIG_SETTING_HORIZONTAL_POS_RIGHT"
msgid "Right"
msgstr "Dreta"

msgctxt "STR_CONFIG_SETTING_SECONDS_VALUE"
msgid "{COMMA}{NBSP}second{P 0 \"\" s}"
msgstr "{COMMA}{NBSP}segon{P 0 \"\" s}"

msgctxt "STR_CONFIG_SETTING_INFINITE_MONEY"
msgid "Infinite money: {STRING2}"
msgstr "Diners infinits: {STRING}"

msgctxt "STR_CONFIG_SETTING_INFINITE_MONEY_HELPTEXT"
msgid "Allow unlimited spending and disable bankruptcy of companies"
msgstr "Permet gastar diners sense límit i desactiva les bancarrotes de les companyies."

msgctxt "STR_CONFIG_SETTING_MAXIMUM_INITIAL_LOAN"
msgid "Maximum initial loan: {STRING2}"
msgstr "Préstec inicial màxim: {STRING}"

msgctxt "STR_CONFIG_SETTING_MAXIMUM_INITIAL_LOAN_HELPTEXT"
msgid "Maximum amount a company can loan (without taking inflation into account). If set to \"No loan\", no money will be available unless provided by a Game Script or the \"Infinite money\" setting"
msgstr "Quantitat màxima del préstec que pot demanar una companyia (sense tenir en compte la inflació). Si s'estableix a \"Sense préstecs\", no es podran aconseguir diners a no ser que els proporcionin els scripts de partida o l'opció \"Diners infinits\"."

msgctxt "STR_CONFIG_SETTING_MAXIMUM_INITIAL_LOAN_DISABLED"
msgid "No loan"
msgstr "Sense préstecs"

msgctxt "STR_CONFIG_SETTING_INTEREST_RATE"
msgid "Interest rate: {STRING2}"
msgstr "Taxa d'interès: {STRING}"

msgctxt "STR_CONFIG_SETTING_INTEREST_RATE_HELPTEXT"
msgid "Loan interest rate; also controls inflation, if enabled"
msgstr "Tipus d'interès dels préstecs; també controla la inflació, si està activada."

msgctxt "STR_CONFIG_SETTING_RUNNING_COSTS"
msgid "Running costs: {STRING2}"
msgstr "Costos d'utilització: {STRING}"

msgctxt "STR_CONFIG_SETTING_RUNNING_COSTS_HELPTEXT"
msgid "Set level of maintenance and running costs of vehicles and infrastructure"
msgstr "Fixa el nivell de manteniment i els costos d'utilització dels vehicles i infraestructures"

msgctxt "STR_CONFIG_SETTING_RUNNING_COSTS_LOW"
msgid "Low"
msgstr "Baix"

msgctxt "STR_CONFIG_SETTING_RUNNING_COSTS_MEDIUM"
msgid "Medium"
msgstr "Mitjà"

msgctxt "STR_CONFIG_SETTING_RUNNING_COSTS_HIGH"
msgid "High"
msgstr "Alt"

msgctxt "STR_CONFIG_SETTING_CONSTRUCTION_SPEED"
msgid "Construction speed: {STRING2}"
msgstr "Ritme de construcció: {STRING}"

msgctxt "STR_CONFIG_SETTING_CONSTRUCTION_SPEED_HELPTEXT"
msgid "Limit the amount of construction actions for AIs"
msgstr "Limita la quantitat d'accions de construcció per part de les IA"

msgctxt "STR_CONFIG_SETTING_VEHICLE_BREAKDOWNS"
msgid "Vehicle breakdowns: {STRING2}"
msgstr "Nombre d'avaries de vehicles: {STRING}"

msgctxt "STR_CONFIG_SETTING_VEHICLE_BREAKDOWNS_HELPTEXT"
msgid "Control how often inadequately serviced vehicles may break down"
msgstr "Controla cada quan els vehicles revisats inadequadament s'espatllen"

msgctxt "STR_CONFIG_SETTING_SUBSIDY_MULTIPLIER"
msgid "Subsidy multiplier: {STRING2}"
msgstr "Multiplicador del subsidi: {STRING}"

msgctxt "STR_CONFIG_SETTING_SUBSIDY_MULTIPLIER_HELPTEXT"
msgid "Set how much is paid for subsidised connections"
msgstr "Fixa quant es paga pels transports subsidiats"

msgctxt "STR_CONFIG_SETTING_SUBSIDY_DURATION"
msgid "Subsidy duration: {STRING2}"
msgstr "Durada dels subsidis: {STRING}"

msgctxt "STR_CONFIG_SETTING_SUBSIDY_DURATION_HELPTEXT"
msgid "Set the number of years for which a subsidy is awarded"
msgstr "Establiu el nombre d'anys que duren els subsidis."

msgctxt "STR_CONFIG_SETTING_SUBSIDY_DURATION_HELPTEXT_PERIODS"
msgid "Set the number of periods for which a subsidy is awarded"
msgstr "Estableix el nombre de períodes que duren les subvencions."

msgctxt "STR_CONFIG_SETTING_SUBSIDY_DURATION_DISABLED"
msgid "No subsidies"
msgstr "Sense subsidis"

msgctxt "STR_CONFIG_SETTING_CONSTRUCTION_COSTS"
msgid "Construction costs: {STRING2}"
msgstr "Costos de construcció: {STRING}"

msgctxt "STR_CONFIG_SETTING_CONSTRUCTION_COSTS_HELPTEXT"
msgid "Set level of construction and purchase costs"
msgstr "Fixa el nivell de construcció i els preus de compra"

msgctxt "STR_CONFIG_SETTING_CONSTRUCTION_COSTS_LOW"
msgid "Low"
msgstr "Baix"

msgctxt "STR_CONFIG_SETTING_CONSTRUCTION_COSTS_MEDIUM"
msgid "Medium"
msgstr "Mitjà"

msgctxt "STR_CONFIG_SETTING_CONSTRUCTION_COSTS_HIGH"
msgid "High"
msgstr "Alt"

msgctxt "STR_CONFIG_SETTING_RECESSIONS"
msgid "Recessions: {STRING2}"
msgstr "Recessions: {STRING}"

msgctxt "STR_CONFIG_SETTING_RECESSIONS_HELPTEXT"
msgid "If enabled, recessions may occur periodically. During a recession all production is significantly lower (it returns to previous level when the recession is over)"
msgstr "Si està activat, poden haver recessions periòdicament. Durant una recessió, tota la producció és significativament més baixa, tornant al nivell previ quan s'acabi el període de recessió."

msgctxt "STR_CONFIG_SETTING_TRAIN_REVERSING"
msgid "Disallow train reversing in stations: {STRING2}"
msgstr "No permetis que els trens canviïn de sentit a les estacions: {STRING}"

msgctxt "STR_CONFIG_SETTING_TRAIN_REVERSING_HELPTEXT"
msgid "If enabled, trains will not reverse in non-terminus stations, even if there is a shorter path to their next destination when reversing"
msgstr "Si està actiu, els trens no canviaran de sentit a les estacions no-finals, inclús si hi ha un camí més curt al seu proper destí en cas de canviar de sentit."

msgctxt "STR_CONFIG_SETTING_DISASTERS"
msgid "Disasters: {STRING2}"
msgstr "Catàstrofes: {STRING}"

msgctxt "STR_CONFIG_SETTING_DISASTERS_HELPTEXT"
msgid "Toggle disasters which may occasionally block or destroy vehicles or infrastructure"
msgstr "Permet o no que passin catàstrofes que puguin bloquejar o destruir ocasionalment vehicles o infraestructures"

msgctxt "STR_CONFIG_SETTING_CITY_APPROVAL"
msgid "Local authority attitude: {STRING2}"
msgstr "Actitud de les alcaldies de les poblacions: {STRING}"

msgctxt "STR_CONFIG_SETTING_CITY_APPROVAL_HELPTEXT"
msgid "Choose how much noise and environmental damage by companies affect their town rating and further construction actions in the town"
msgstr "Establiu com afecta al prestigi de les companyies el soroll i els danys ambientals que provoquen. Aquest prestigi influirà en la futura acceptació o denegació d'accions de construcció a la població."

msgctxt "STR_CONFIG_SETTING_MAP_HEIGHT_LIMIT"
msgid "Map height limit: {STRING2}"
msgstr "Alçada màxima del mapa: {STRING}"

msgctxt "STR_CONFIG_SETTING_MAP_HEIGHT_LIMIT_HELPTEXT"
msgid "Set the maximum height of the map terrain. With \"(auto)\" a good value will be picked after terrain generation"
msgstr "Estableix l'alçada màxima del terreny del mapa. Amb «Automàtica», s'escollirà un valor adequat després que es generi el terreny."

msgctxt "STR_CONFIG_SETTING_MAP_HEIGHT_LIMIT_AUTO"
msgid "(auto)"
msgstr "Automàtica"

msgctxt "STR_CONFIG_SETTING_TOO_HIGH_MOUNTAIN"
msgid "You can't set the map height limit to this value. At least one mountain on the map is higher"
msgstr "{WHITE}No es pot escollir aquest límit d'alçada del mapa. Hi ha almenys una muntanya al mapa que és més alta."

msgctxt "STR_CONFIG_SETTING_AUTOSLOPE"
msgid "Allow landscaping under buildings, tracks, etc.: {STRING2}"
msgstr "Permet la modificació del terreny sota edificis, rails, etc.: {STRING}"

msgctxt "STR_CONFIG_SETTING_AUTOSLOPE_HELPTEXT"
msgid "Allow landscaping under buildings and tracks without removing them"
msgstr "Permet modelar el terreny sota les vies, els edificis i altres elements sense eliminar-los."

msgctxt "STR_CONFIG_SETTING_CATCHMENT"
msgid "Allow more realistically sized catchment areas: {STRING2}"
msgstr "Estableix una mida més real de l'àrea d'influència: {STRING}"

msgctxt "STR_CONFIG_SETTING_CATCHMENT_HELPTEXT"
msgid "Have differently sized catchment areas for different types of stations and airports"
msgstr "Permet tenir diferents mides de zones de captació de càrrega per diferents tipus d'estacions (aeroports, estacions de tren, etc.)."

msgctxt "STR_CONFIG_SETTING_SERVE_NEUTRAL_INDUSTRIES"
msgid "Company stations can serve industries with attached neutral stations: {STRING2}"
msgstr "Les estacions de la companyia poden donar servei a industries amb estacions neutrals: {STRING}"

msgctxt "STR_CONFIG_SETTING_SERVE_NEUTRAL_INDUSTRIES_HELPTEXT"
msgid "When enabled, industries with attached stations (such as Oil Rigs) may also be served by company-owned stations built nearby. When disabled, these industries may only be served by their attached stations. Any nearby company stations won't be able to serve them, nor will the attached station serve anything else other than the industry"
msgstr "Quan s'activa, les indústries amb estacions pròpies, com ara les plataformes petrolíferes, es poden servir amb estacions properes de les companyies. Quan es desactiva, aquestes indústries només es poden servir amb les estacions de la indústria: qualsevol estació propera que no sigui la de l'estació no la podrà servir, ni l'estació de la indústria podrà servir res que no provingui d'aquesta indústria."

msgctxt "STR_CONFIG_SETTING_EXTRADYNAMITE"
msgid "Allow removal of more town-owned roads, bridges and tunnels: {STRING2}"
msgstr "Permet l'esborrat de carreteres, ponts i túnels propietat de les poblacions: {STRING}"

msgctxt "STR_CONFIG_SETTING_EXTRADYNAMITE_HELPTEXT"
msgid "Make it easier to remove town-owned infrastructure and buildings"
msgstr "Aquesta opció permet fer més fàcil l'esborrat d'infraestructures i construccions propietat de la població"

msgctxt "STR_CONFIG_SETTING_TRAIN_LENGTH"
msgid "Maximum length of trains: {STRING2}"
msgstr "Longitud màxima dels trens: {STRING}"

msgctxt "STR_CONFIG_SETTING_TRAIN_LENGTH_HELPTEXT"
msgid "Set the maximum length of trains"
msgstr "Estableix la longitud màxima dels trens"

msgctxt "STR_CONFIG_SETTING_TILE_LENGTH"
msgid "{COMMA} tile{P 0 \"\" s}"
msgstr "{COMMA} cel·l{P a es}"

msgctxt "STR_CONFIG_SETTING_SMOKE_AMOUNT"
msgid "Amount of vehicle smoke/sparks: {STRING2}"
msgstr "Quantitat de fum i espurnes dels vehicles: {STRING}"

msgctxt "STR_CONFIG_SETTING_SMOKE_AMOUNT_HELPTEXT"
msgid "Set how much smoke or how many sparks are emitted by vehicles"
msgstr "Estableix la quantitat de fum o quantes espurnes són emeses pels vehicles"

msgctxt "STR_CONFIG_SETTING_TRAIN_ACCELERATION_MODEL"
msgid "Train acceleration model: {STRING2}"
msgstr "Model d'acceleració dels trens: {STRING}"

msgctxt "STR_CONFIG_SETTING_TRAIN_ACCELERATION_MODEL_HELPTEXT"
msgid "Select the physics model for train acceleration. The \"original\" model penalises slopes equally for all vehicles. The \"realistic\" model penalises slopes and curves depending on various properties of the consist, like length and tractive effort"
msgstr "Seleccioneu el model físic per l'acceleració de trens. El model «original» penalitza les pujades per igual a tots els vehicles. El model «realista» penalitza les pujades i les corbes en funció de diverses propietats del tren, com ara la longitud i la força de tracció"

msgctxt "STR_CONFIG_SETTING_ROAD_VEHICLE_ACCELERATION_MODEL"
msgid "Road vehicle acceleration model: {STRING2}"
msgstr "Model d'acceleració per als vehicles: {STRING}"

msgctxt "STR_CONFIG_SETTING_ROAD_VEHICLE_ACCELERATION_MODEL_HELPTEXT"
msgid "Select the physics model for road vehicle acceleration. The \"original\" model penalises slopes equally for all vehicles. The \"realistic\" model penalises slopes depending on various properties of the engine, for example 'tractive effort'"
msgstr "Seleccioneu el model físic per l'acceleració d'automòbils. El model «original» penalitza les pujades per igual a tots els vehicles. El model «realista» penalitza els pendents en funció de les diferents propietats de la màquina, com per exemple l'esforç de tracció."

msgctxt "STR_CONFIG_SETTING_TRAIN_SLOPE_STEEPNESS"
msgid "Slope steepness for trains: {STRING2}"
msgstr "Pendent de les costes per als trens: {STRING}"

msgctxt "STR_CONFIG_SETTING_TRAIN_SLOPE_STEEPNESS_HELPTEXT"
msgid "Steepness of a sloped tile for a train. Higher values make it more difficult to climb a hill"
msgstr "El pendent de les caselles amb costes per als trens. Els valors alts fan que sigui més difícil pujar els turons."

msgctxt "STR_CONFIG_SETTING_PERCENTAGE"
msgid "{COMMA}%"
msgstr "{COMMA}{NBSP}%"

msgctxt "STR_CONFIG_SETTING_ROAD_VEHICLE_SLOPE_STEEPNESS"
msgid "Slope steepness for road vehicles: {STRING2}"
msgstr "Pendent de les costes per als vehicles de carretera: {STRING}"

msgctxt "STR_CONFIG_SETTING_ROAD_VEHICLE_SLOPE_STEEPNESS_HELPTEXT"
msgid "Steepness of a sloped tile for a road vehicle. Higher values make it more difficult to climb a hill"
msgstr "El pendent de les caselles amb costes per a vehicles de carretera. Els valors alts fan que sigui més difícil pujar els turons."

msgctxt "STR_CONFIG_SETTING_FORBID_90_DEG"
msgid "Forbid trains from making 90° turns: {STRING2}"
msgstr "Prohibeix fer girs de 90 graus als trens: {STRING}"

msgctxt "STR_CONFIG_SETTING_FORBID_90_DEG_HELPTEXT"
msgid "90 degree turns occur when a horizontal track is directly followed by a vertical track piece on the adjacent tile, thus making the train turn by 90 degree when traversing the tile edge instead of the usual 45 degrees for other track combinations"
msgstr "Els girs de 90 graus succeeixen quan hi ha una via horitzontal seguida d'una de vertical a la cel·la annexa, provocant que el tren giri 90 graus quan travessi la vora de la cel·la en lloc dels 45 graus usuals en les altres combinacions."

msgctxt "STR_CONFIG_SETTING_DISTANT_JOIN_STATIONS"
msgid "Allow to join stations not directly adjacent: {STRING2}"
msgstr "Permet ajuntar estacions no annexes: {STRING}"

msgctxt "STR_CONFIG_SETTING_DISTANT_JOIN_STATIONS_HELPTEXT"
msgid "Allow adding parts to a station without directly touching the existing parts, by Ctrl+Clicking while placing the new parts"
msgstr "Aquesta opció permet afegir parts noves a una estació existent sense estar les parts noves en contacte directe amb les existents. Cal fer Ctrl+clic quan es col·loquen les parts noves."

msgctxt "STR_CONFIG_SETTING_INFLATION"
msgid "Inflation: {STRING2}"
msgstr "Inflació: {STRING}"

msgctxt "STR_CONFIG_SETTING_INFLATION_HELPTEXT"
msgid "Enable inflation in the economy, where costs are slightly faster rising than payments"
msgstr "Habilita la inflació a l'economia, on l'augment dels costos són una mica més ràpids que els dels pagaments."

msgctxt "STR_CONFIG_SETTING_MAX_BRIDGE_LENGTH"
msgid "Maximum bridge length: {STRING2}"
msgstr "Longitud màxima dels ponts: {STRING}"

msgctxt "STR_CONFIG_SETTING_MAX_BRIDGE_LENGTH_HELPTEXT"
msgid "Maximum length for building bridges"
msgstr "Longitud màxima per a la construcció de ponts"

msgctxt "STR_CONFIG_SETTING_MAX_BRIDGE_HEIGHT"
msgid "Maximum bridge height: {STRING2}"
msgstr "Alçada màxima dels ponts: {STRING}"

msgctxt "STR_CONFIG_SETTING_MAX_BRIDGE_HEIGHT_HELPTEXT"
msgid "Maximum height for building bridges"
msgstr "Alçada màxima de construcció dels ponts"

msgctxt "STR_CONFIG_SETTING_MAX_TUNNEL_LENGTH"
msgid "Maximum tunnel length: {STRING2}"
msgstr "Màxima longitud dels túnels: {STRING}"

msgctxt "STR_CONFIG_SETTING_MAX_TUNNEL_LENGTH_HELPTEXT"
msgid "Maximum length for building tunnels"
msgstr "Longitud màxima per a la construcció de túnels"

msgctxt "STR_CONFIG_SETTING_RAW_INDUSTRY_CONSTRUCTION_METHOD"
msgid "Manual primary industry construction method: {STRING2}"
msgstr "Fundació de noves indústries primàries: {STRING}"

msgctxt "STR_CONFIG_SETTING_RAW_INDUSTRY_CONSTRUCTION_METHOD_HELPTEXT"
msgid "Method of funding a primary industry. 'none' means it is not possible to fund any, 'prospecting' means funding is possible, but construction occurs in a random spot on the map and may as well fail, 'as other industries' means raw industries can be constructed by companies like processing industries in any position they like"
msgstr "Mètode de fundació de noves indústries primàries. «Fundació desactivada» significa que no serà possible fundar-ne cap, «Amb prospeccions» significa que serà possible fundar-ne, però la construcció succeirà en un lloc aleatori del mapa i podria fracassar. «Com les altres indústries» significa que les indústries de matèries primeres poden ser construïdes en qualsevol lloc per les companyies igual que les indústries manufactureres."

msgctxt "STR_CONFIG_SETTING_RAW_INDUSTRY_CONSTRUCTION_METHOD_NONE"
msgid "None"
msgstr "Fundació desactivada"

msgctxt "STR_CONFIG_SETTING_RAW_INDUSTRY_CONSTRUCTION_METHOD_NORMAL"
msgid "As other industries"
msgstr "Com les altres indústries"

msgctxt "STR_CONFIG_SETTING_RAW_INDUSTRY_CONSTRUCTION_METHOD_PROSPECTING"
msgid "Prospecting"
msgstr "Amb prospeccions"

msgctxt "STR_CONFIG_SETTING_INDUSTRY_PLATFORM"
msgid "Flat area around industries: {STRING2}"
msgstr "Àrea anivellada al voltant de les indústries: {STRING}"

msgctxt "STR_CONFIG_SETTING_INDUSTRY_PLATFORM_HELPTEXT"
msgid "Amount of flat space around an industry. This ensures empty space will remain available around an industry for building tracks, et cetera"
msgstr "Quantitat d'espai pla al voltant d'una indústria. Això assegura que quedarà espai buit al voltant de la indústria per construir vies o altres infraestructures."

msgctxt "STR_CONFIG_SETTING_MULTIPINDTOWN"
msgid "Allow multiple similar industries per town: {STRING2}"
msgstr "Permet vàries indústries similars a cada població: {STRING}"

msgctxt "STR_CONFIG_SETTING_MULTIPINDTOWN_HELPTEXT"
msgid "Normally, a town does not want more than one industry of each type. With this setting, it will allow several industries of the same type in the same town"
msgstr "Normalment, una població no vol tenir més d'una indústria de cada tipus. Amb aquest paràmetre es permet la construcció de diverses indústries del mateix tipus a la mateixa població."

msgctxt "STR_CONFIG_SETTING_SIGNALSIDE"
msgid "Show signals: {STRING2}"
msgstr "Mostra els senyals: {STRING}"

msgctxt "STR_CONFIG_SETTING_SIGNALSIDE_HELPTEXT"
msgid "Select on which side of the track to place signals"
msgstr "Seleccioneu a quin costat de la via s'han de posar els senyals"

msgctxt "STR_CONFIG_SETTING_SIGNALSIDE_LEFT"
msgid "On the left"
msgstr "A l'esquerra"

msgctxt "STR_CONFIG_SETTING_SIGNALSIDE_DRIVING_SIDE"
msgid "On the driving side"
msgstr "Al costat de conducció"

msgctxt "STR_CONFIG_SETTING_SIGNALSIDE_RIGHT"
msgid "On the right"
msgstr "A la dreta"

msgctxt "STR_CONFIG_SETTING_SHOWFINANCES"
msgid "Show finances window at the end of the year: {STRING2}"
msgstr "Mostra la finestra de balanç al finalitzar cada any: {STRING}"

msgctxt "STR_CONFIG_SETTING_SHOWFINANCES_PERIOD"
msgid "Show finances window at the end of the period: {STRING2}"
msgstr "Mostra la finestra de finances al final d'aquest període: {STRING}"

msgctxt "STR_CONFIG_SETTING_SHOWFINANCES_HELPTEXT"
msgid "If enabled, the finances window pops up at the end of each year to allow easy inspection of the financial status of the company"
msgstr "Si el paràmetre està activat, la finestra de finances apareix al final de cada any per permetre una inspecció fàcil de l'estat financer de la companyia."

msgctxt "STR_CONFIG_SETTING_SHOWFINANCES_HELPTEXT_PERIOD"
msgid "If enabled, the finances window pops up at the end of each period to allow easy inspection of the financial status of the company"
msgstr "Si el paràmetre està activat, la finestra de finances apareix al final de cada període per a permetre una inspecció fàcil de l'estat financer de la companyia."

msgctxt "STR_CONFIG_SETTING_NONSTOP_BY_DEFAULT"
msgid "New orders are 'non-stop' by default: {STRING2}"
msgstr "«Sense parada» per defecte a les ordres noves: {STRING}"

msgctxt "STR_CONFIG_SETTING_NONSTOP_BY_DEFAULT_HELPTEXT"
msgid "Normally, a vehicle will stop at every station it passes. By enabling this setting, it will drive through all station on the way to its final destination without stopping. Note, that this setting only defines a default value for new orders. Individual orders can be set explicitly to either behaviour nevertheless"
msgstr "Normalment, un vehicle para a cada estació per la que passa. Activant aquest paràmetre, no pararà a cap estació fins arribar al seu destí. Nota: aquest paràmetre només defineix el valor predeterminat per a les noves ordres. Un cop creada una ordre, aquesta es pot establir manualment al comportament desitjat."

msgctxt "STR_CONFIG_SETTING_STOP_LOCATION"
msgid "New train orders stop by default at the {STRING2} of the platform"
msgstr "Noves ordres de trens: parar per defecte {STRING} de la plataforma"

msgctxt "STR_CONFIG_SETTING_STOP_LOCATION_HELPTEXT"
msgid "Place where a train will stop at the platform by default. The 'near end' means close to the entry point, 'middle' means in the middle of the platform, and 'far end' means far away from the entry point. Note, that this setting only defines a default value for new orders. Individual orders can have their stop location set by clicking on the order text"
msgstr "Estableix en quin lloc de la plataforma els trens pararan per defecte. «Extrem més proper» significa a prop del punt d'entrada, «Al mig» significa al mig de la plataforma, i «Extrem més llunyà» significa lluny del punt d'entrada. Nota: aquest paràmetre només defineix el valor predeterminat per les noves ordres. Un cop creada una ordre, es pot modificar el lloc de parada a la plataforma si es fa clic al text de l'ordre."

msgctxt "STR_CONFIG_SETTING_STOP_LOCATION_NEAR_END"
msgid "near end"
msgstr "A l'extrem més proper"

msgctxt "STR_CONFIG_SETTING_STOP_LOCATION_MIDDLE"
msgid "middle"
msgstr "Al mig"

msgctxt "STR_CONFIG_SETTING_STOP_LOCATION_FAR_END"
msgid "far end"
msgstr "A l'extrem més llunyà"

msgctxt "STR_CONFIG_SETTING_AUTOSCROLL"
msgid "Pan window when mouse is at the edge: {STRING2}"
msgstr "Desplaça la finestra quan el ratolí sigui prop de la vora: {STRING}"

msgctxt "STR_CONFIG_SETTING_AUTOSCROLL_HELPTEXT"
msgid "When enabled, viewports will start to scroll when the mouse is near the edge of the window"
msgstr "Si s'activa aquesta opció, l'àrea de visualització començarà a desplaçar-se quan el ratolí sigui prop de la vora de la finestra."

msgctxt "STR_CONFIG_SETTING_AUTOSCROLL_DISABLED"
msgid "Disabled"
msgstr "Desactivat"

msgctxt "STR_CONFIG_SETTING_AUTOSCROLL_MAIN_VIEWPORT_FULLSCREEN"
msgid "Main viewport, full-screen only"
msgstr "Vista principal (només pantalla completa)"

msgctxt "STR_CONFIG_SETTING_AUTOSCROLL_MAIN_VIEWPORT"
msgid "Main viewport"
msgstr "Vista principal"

msgctxt "STR_CONFIG_SETTING_AUTOSCROLL_EVERY_VIEWPORT"
msgid "Every viewport"
msgstr "Qualsevol vista"

msgctxt "STR_CONFIG_SETTING_BRIBE"
msgid "Allow bribing of the local authority: {STRING2}"
msgstr "Permet subornar les autoritats locals: {STRING}"

msgctxt "STR_CONFIG_SETTING_BRIBE_HELPTEXT"
msgid "Allow companies to try bribing the local town authority. If the bribe is noticed by an inspector, the company will not be able to act in the town for six months"
msgstr "Permet que les companyies subornin l'autoritat local. Si algun inspector ho detecta, la companyia no podrà actuar a la població durant sis mesos."

msgctxt "STR_CONFIG_SETTING_BRIBE_HELPTEXT_MINUTES"
msgid "Allow companies to try bribing the local town authority. If the bribe is noticed by an inspector, the company will not be able to act in the town for six minutes"
msgstr "Permet que les companyies subornin l'autoritat local. Si algun inspector ho detecta, la companyia no podrà actuar a la població durant sis minuts."

msgctxt "STR_CONFIG_SETTING_ALLOW_EXCLUSIVE"
msgid "Allow buying exclusive transport rights: {STRING2}"
msgstr "Permet comprar els drets del transport en exclusiva: {STRING}"

msgctxt "STR_CONFIG_SETTING_ALLOW_EXCLUSIVE_HELPTEXT"
msgid "If a company buys exclusive transport rights for a town, opponents' stations (passenger and cargo) won't receive any cargo for twelve months"
msgstr "Si una companyia compra els drets de transport en exclusiva d'una població, les estacions dels oponents (passatgers i càrrega) no rebran cap càrrega durant dotze mesos."

msgctxt "STR_CONFIG_SETTING_ALLOW_EXCLUSIVE_HELPTEXT_MINUTES"
msgid "If a company buys exclusive transport rights for a town, opponents' stations (passenger and cargo) won't receive any cargo for twelve minutes"
msgstr "Si una companyia compra els drets de transport en exclusiva d'una població, les estacions dels oponents (passatgers i càrrega) no rebran cap càrrega durant dotze minuts."

msgctxt "STR_CONFIG_SETTING_ALLOW_FUND_BUILDINGS"
msgid "Allow funding buildings: {STRING2}"
msgstr "Permet finançar edificis: {STRING}"

msgctxt "STR_CONFIG_SETTING_ALLOW_FUND_BUILDINGS_HELPTEXT"
msgid "Allow companies to give money to towns for funding new houses"
msgstr "Permet a les companyies donar diners a les poblacions per finançar nous edificis."

msgctxt "STR_CONFIG_SETTING_ALLOW_FUND_ROAD"
msgid "Allow funding local road reconstruction: {STRING2}"
msgstr "Permet finançar la reconstrucció dels carrers locals: {STRING}"

msgctxt "STR_CONFIG_SETTING_ALLOW_FUND_ROAD_HELPTEXT"
msgid "Allow companies to give money to towns for road re-construction to sabotage road-based services in the town"
msgstr "Permet a les companyies donar diners a les poblacions per la reconstrucció de carrers i sabotejar la circulació de vehicles."

msgctxt "STR_CONFIG_SETTING_ALLOW_GIVE_MONEY"
msgid "Allow sending money to other companies: {STRING2}"
msgstr "Permet enviar diners a altres companyies: {STRING}"

msgctxt "STR_CONFIG_SETTING_ALLOW_GIVE_MONEY_HELPTEXT"
msgid "Allow transfer of money between companies in multiplayer mode"
msgstr "Permet transferir diners entre companyies en el mode multijugador."

msgctxt "STR_CONFIG_SETTING_FREIGHT_TRAINS"
msgid "Weight multiplier for freight to simulate heavy trains: {STRING2}"
msgstr "Multiplicador de pes per simular trens pesats: {STRING}"

msgctxt "STR_CONFIG_SETTING_FREIGHT_TRAINS_HELPTEXT"
msgid "Set the impact of carrying freight in trains. A higher value makes carrying freight more demanding for trains, especially at hills"
msgstr "Estableix l'impacte de transportar contenidors en trens. Els valors alts fan que transportar contenidors sigui més exigent amb els trens, especialment als turons."

msgctxt "STR_CONFIG_SETTING_PLANE_SPEED"
msgid "Plane speed factor: {STRING2}"
msgstr "Factor de velocitat dels avions: {STRING}"

msgctxt "STR_CONFIG_SETTING_PLANE_SPEED_HELPTEXT"
msgid "Set the relative speed of planes compared to other vehicle types, to reduce the amount of income of transport by aircraft"
msgstr "Estableix la velocitat relativa dels avions en comparació amb els altres tipus de vehicles, per reduir la quantitat de guanys de transport dels avions"

msgctxt "STR_CONFIG_SETTING_PLANE_CRASHES"
msgid "Number of plane crashes: {STRING2}"
msgstr "Nombre d'accidents d'avió: {STRING}"

msgctxt "STR_CONFIG_SETTING_PLANE_CRASHES_HELPTEXT"
msgid "Set the chance of a random aircraft crash happening.{}* Large airplanes always have a risk of crashing when landing on small airports"
msgstr "Estableix la probabilitat amb què succeeixen els accidents d'avió.{}* Els avions grans sempre tenen un risc d'estavellar-se quan aterren en aeroports petits."

msgctxt "STR_CONFIG_SETTING_PLANE_CRASHES_NONE"
msgid "None*"
msgstr "Cap*"

msgctxt "STR_CONFIG_SETTING_PLANE_CRASHES_REDUCED"
msgid "Reduced"
msgstr "Reduït"

msgctxt "STR_CONFIG_SETTING_CROSSING_WITH_COMPETITOR"
msgid "Allow level crossings with roads or rails owned by competitors: {STRING2}"
msgstr "Permet passos a nivell amb carreteres o rails dels competidors: {STRING}"

msgctxt "STR_CONFIG_SETTING_CROSSING_WITH_COMPETITOR_HELPTEXT"
msgid "Allow construction of level crossings on roads or rails owned by competitors"
msgstr "Permet la construcció de passos a nivell en carreteres o vies ferroviàries que siguin propietat d'altres jugadors."

msgctxt "STR_CONFIG_SETTING_STOP_ON_TOWN_ROAD"
msgid "Allow drive-through road stops on roads owned by towns: {STRING2}"
msgstr "Permet situar parades en carreteres que són propietat de les poblacions: {STRING}"

msgctxt "STR_CONFIG_SETTING_STOP_ON_TOWN_ROAD_HELPTEXT"
msgid "Allow construction of drive-through road stops on roads owned by towns"
msgstr "Permet la construcció de passos a nivell als carrers propietat de les poblacions"

msgctxt "STR_CONFIG_SETTING_STOP_ON_COMPETITOR_ROAD"
msgid "Allow drive-through road stops on roads owned by competitors: {STRING2}"
msgstr "Permet circular a través de les parades en carreteres propietat de competidors: {STRING}"

msgctxt "STR_CONFIG_SETTING_STOP_ON_COMPETITOR_ROAD_HELPTEXT"
msgid "Allow construction of drive-through road stops on roads owned by other companies"
msgstr "Permet la construcció de passos a nivell en els carrers propietat d'altres companyies"

msgctxt "STR_CONFIG_SETTING_DYNAMIC_ENGINES_EXISTING_VEHICLES"
msgid "Changing this setting is not possible when there are vehicles"
msgstr "{WHITE}No es pot canviar aquest ajustament quan hi ha vehicles"

msgctxt "STR_CONFIG_SETTING_INFRASTRUCTURE_MAINTENANCE"
msgid "Infrastructure maintenance: {STRING2}"
msgstr "Manteniment de la infraestructura: {STRING}"

msgctxt "STR_CONFIG_SETTING_INFRASTRUCTURE_MAINTENANCE_HELPTEXT"
msgid "When enabled, infrastructure causes maintenance costs. The cost grows over-proportional with the network size, thus affecting bigger companies more than smaller ones"
msgstr "Si està activat, s'han de pagar costos de manteniment per infraestructures. El cost creix proporcionalment amb la mida de la xarxa de transport i, en conseqüència, afecta més a les companyies grosses que a les petites."

msgctxt "STR_CONFIG_SETTING_COMPANY_STARTING_COLOUR"
msgid "Starting company colour: {STRING2}"
msgstr "Color inicial de la companyia: {STRING}"

msgctxt "STR_CONFIG_SETTING_COMPANY_STARTING_COLOUR_HELPTEXT"
msgid "Choose starting colour for the company"
msgstr "Escolliu el color inicial de la vostra companyia."

msgctxt "STR_CONFIG_SETTING_COMPANY_STARTING_COLOUR_SECONDARY"
msgid "Starting company secondary colour: {STRING2}"
msgstr "Color secundari inicial de la companyia: {STRING}"

msgctxt "STR_CONFIG_SETTING_COMPANY_STARTING_COLOUR_SECONDARY_HELPTEXT"
msgid "Choose starting secondary colour for the company, if using a NewGRF that enables it"
msgstr "Escolliu un color secundari inicial per a la companyia, per si feu servir un NewGRF que l'habiliti."

msgctxt "STR_CONFIG_SETTING_NEVER_EXPIRE_AIRPORTS"
msgid "Airports never expire: {STRING2}"
msgstr "Els aeroports mai caduquen: {STRING}"

msgctxt "STR_CONFIG_SETTING_NEVER_EXPIRE_AIRPORTS_HELPTEXT"
msgid "Enabling this setting makes each airport type stay available forever after its introduction"
msgstr "Activant aquesta opció fa que cada tipus d'aeroport estigui disponible per sempre després de la seva introducció"

msgctxt "STR_CONFIG_SETTING_WARN_LOST_VEHICLE"
msgid "Warn if vehicle is lost: {STRING2}"
msgstr "Avisa si el vehicle es perd: {STRING}"

msgctxt "STR_CONFIG_SETTING_WARN_LOST_VEHICLE_HELPTEXT"
msgid "Trigger messages about vehicles unable to find a path to their ordered destination"
msgstr "Alerta amb missatges quan els vehicles no siguin capaços de trobar el camí al seu destí indicat"

msgctxt "STR_CONFIG_SETTING_ORDER_REVIEW"
msgid "Review vehicles' orders: {STRING2}"
msgstr "Revisa ordres dels vehicles: {STRING}"

msgctxt "STR_CONFIG_SETTING_ORDER_REVIEW_HELPTEXT"
msgid "When enabled, the orders of the vehicles are periodically checked, and some obvious issues are reported with a news message when detected"
msgstr "Si està activat, les ordres dels vehicles es verifiquen periòdicament i, si es detecten problemes evidents, s'informarà a través de missatges de notícia."

msgctxt "STR_CONFIG_SETTING_ORDER_REVIEW_EXDEPOT"
msgid "Yes, but exclude stopped vehicles"
msgstr "Sí, excepte vehicles parats"

msgctxt "STR_CONFIG_SETTING_ORDER_REVIEW_ON"
msgid "Of all vehicles"
msgstr "De tots els vehicles"

msgctxt "STR_CONFIG_SETTING_WARN_INCOME_LESS"
msgid "Warn if a vehicle's income is negative: {STRING2}"
msgstr "Avisa si el benefici del vehicle és negatiu: {STRING}"

msgctxt "STR_CONFIG_SETTING_WARN_INCOME_LESS_HELPTEXT"
msgid "When enabled, a news message gets sent when a vehicle has not made any profit within a year"
msgstr "Si està activat, s'envia un missatge de notícia quan un vehicle no ha tingut beneficis durant l'any."

msgctxt "STR_CONFIG_SETTING_WARN_INCOME_LESS_HELPTEXT_PERIOD"
msgid "When enabled, a news message gets sent when a vehicle has not made any profit within a period"
msgstr "Si està activat, s'envia un missatge de notícia quan un vehicle no ha obtingut benefici durant un període."

msgctxt "STR_CONFIG_SETTING_WARN_OLD_VEHICLE"
msgid "Warn if a vehicle is getting old: {STRING2}"
msgstr "Avisa si el vehicle és vell: {STRING}"

msgctxt "STR_CONFIG_SETTING_WARN_OLD_VEHICLE_HELPTEXT"
msgid "When enabled, a news message gets sent when a vehicle is getting old"
msgstr "Si està activat, s'envia un missatge de notícia quan un vehicle és vell."

msgctxt "STR_CONFIG_SETTING_NEVER_EXPIRE_VEHICLES"
msgid "Vehicles never expire: {STRING2}"
msgstr "Els vehicles mai caduquen: {STRING}"

msgctxt "STR_CONFIG_SETTING_NEVER_EXPIRE_VEHICLES_HELPTEXT"
msgid "When enabled, all vehicle models remain available forever after their introduction"
msgstr "Si està activat, tots els models de vehicles continuen disponibles per sempre després de la seva introducció."

msgctxt "STR_CONFIG_SETTING_TIMEKEEPING_UNITS"
msgid "Timekeeping: {STRING2}"
msgstr "Mode temporal: {STRING}"

msgctxt "STR_CONFIG_SETTING_TIMEKEEPING_UNITS_HELPTEXT"
msgid "Select the timekeeping units of the game. This cannot be changed later.{}{}Calendar-based is the classic OpenTTD experience, with a year consisting of 12 months, and each month having 28-31 days.{}{}In Wallclock-based time, cargo production and financials are instead based on one-minute increments, which is about as long as a 30 day month takes in Calendar-based mode. These are grouped into 12-minute periods, equivalent to a year in Calendar-based mode.{}{}In either mode there is always a classic calendar, which is used for introduction dates of vehicles, houses, and other infrastructure"
msgstr "Trieu les unitats del mode temporal de la partida. Una vegada començada la partida, no es pot canviar.{}{}El mode calendari ofereix l'experiència clàssica de l'OpenTTD, on un any té 12 mesos i cada mes té entre 28 i 31 dies.{}{}Amb el mode de rellotge de paret, la producció de càrrega i les finances es basen en increments d'un minut, que equival més o menys a un mes d'uns 30 dies en el mode calendari.{}{}En tots els modes sempre hi ha un calendari clàssic, que es fa servir per a la data d'introducció dels vehicles, cases i altres infraestructures."

msgctxt "STR_CONFIG_SETTING_TIMEKEEPING_UNITS_CALENDAR"
msgid "Calendar"
msgstr "Calendari"

msgctxt "STR_CONFIG_SETTING_TIMEKEEPING_UNITS_WALLCLOCK"
msgid "Wallclock"
msgstr "Rellotge de paret"

msgctxt "STR_CONFIG_SETTING_MINUTES_PER_YEAR"
msgid "Minutes per year: {STRING2}"
msgstr "Minuts per any: {STRING}"

msgctxt "STR_CONFIG_SETTING_MINUTES_PER_YEAR_HELPTEXT"
msgid "Choose the number of minutes in a calendar year. The default is 12 minutes. Set to 0 to stop calendar time from changing. Increasing the length of the calendar year slows down the introduction of vehicles, houses, and other infrastructure. It does not affect vehicle speed or economic simulation, except for inflation. This setting is only available when using wallclock timekeeping"
msgstr "Trieu el nombre de minuts d'un any de calendari. Per defecte són 12 minuts. Establiu-ho a 0 per a parar el temps del calendari. Si s'augmenta la durada de l'any del calendari s'alenteix la introducció de vehicles, cases i altres tipus d'infraestructures noves. No afecta a la velocitat dels vehicles o de la simulació econòmica, excepte per la inflació. Aquesta opció només està disponible quan es fa servir el temps amb rellotge de paret."

msgctxt "STR_CONFIG_SETTING_MINUTES_PER_YEAR_FROZEN"
msgid "0 (calendar time frozen)"
msgstr "0 (temps de calendari parat)"

msgctxt "STR_CONFIG_SETTING_TOWN_CARGO_SCALE"
msgid "Scale town cargo production: {STRING2}"
msgstr "Escala la producció de càrrega de les poblacions: {STRING}"

msgctxt "STR_CONFIG_SETTING_TOWN_CARGO_SCALE_HELPTEXT"
msgid "Scale the cargo production of towns by this percentage"
msgstr "Escala la producció de càrrega de les poblacions segons aquest percentatge."

msgctxt "STR_CONFIG_SETTING_INDUSTRY_CARGO_SCALE"
msgid "Scale industry cargo production: {STRING2}"
msgstr "Escala la producció de càrrega de les indústries: {STRING}"

msgctxt "STR_CONFIG_SETTING_INDUSTRY_CARGO_SCALE_HELPTEXT"
msgid "Scale the cargo production of industries by this percentage"
msgstr "Escala la producció de càrrega de les indústries segons aquest percentatge."

msgctxt "STR_CONFIG_SETTING_CARGO_SCALE_VALUE"
msgid "{NUM}%"
msgstr "{NUM}{NBSP}%"

msgctxt "STR_CONFIG_SETTING_AUTORENEW_VEHICLE"
msgid "Autorenew vehicle when it gets old: {STRING2}"
msgstr "Autorenova el vehicle quan sigui vell: {STRING}"

msgctxt "STR_CONFIG_SETTING_AUTORENEW_VEHICLE_HELPTEXT"
msgid "When enabled, a vehicle nearing its end of life gets automatically replaced when the renew conditions are fulfilled"
msgstr "Si està activat, quan un vehicle arriba al final de la seva vida útil es substitueix automàticament quan es compleixen les condicions d'autorenovació."

msgctxt "STR_CONFIG_SETTING_AUTORENEW_MONTHS"
msgid "Autorenew when vehicle is {STRING2} maximum age"
msgstr "Autorenova el vehicle {STRING} de la seva edat màxima."

msgctxt "STR_CONFIG_SETTING_AUTORENEW_MONTHS_HELPTEXT"
msgid "Relative age when a vehicle should be considered for auto-renewing"
msgstr "Edat a partir de la qual s'ha de considerar l'autorenovació del vehicle."

msgctxt "STR_CONFIG_SETTING_AUTORENEW_MONTHS_VALUE_BEFORE"
msgid "{COMMA} month{P 0 \"\" s} before"
msgstr "{COMMA} mes{P 0 \"\" os} abans"

msgctxt "STR_CONFIG_SETTING_AUTORENEW_MONTHS_VALUE_AFTER"
msgid "{COMMA} month{P 0 \"\" s} after"
msgstr "{COMMA} mes{P 0 \"\" os} després"

msgctxt "STR_CONFIG_SETTING_AUTORENEW_MONEY"
msgid "Autorenew minimum needed money for renew: {STRING2}"
msgstr "Mínim de diners per a renovacions automàtiques: {STRING}"

msgctxt "STR_CONFIG_SETTING_AUTORENEW_MONEY_HELPTEXT"
msgid "Minimal amount of money that must remain in the bank before considering auto-renewing vehicles"
msgstr "Estableix la quantitat mínima de diners que han de quedar al banc abans de considerar l'autorenovació de vehicles."

msgctxt "STR_CONFIG_SETTING_ERRMSG_DURATION"
msgid "Duration of error message: {STRING2}"
msgstr "Duració del missatge d'error: {STRING}"

msgctxt "STR_CONFIG_SETTING_ERRMSG_DURATION_HELPTEXT"
msgid "Duration for displaying error messages in a red window. Note that some (critical) error messages are not closed automatically after this time, but must be closed manually"
msgstr "Duració dels missatges d'error mostrats en finestres vermelles. Alguns missatges d'error (crítics) no es tanquen automàticament i s'han de tancar manualment"

msgctxt "STR_CONFIG_SETTING_HOVER_DELAY"
msgid "Show tooltips: {STRING2}"
msgstr "Mostra descripcions emergents: {STRING}"

msgctxt "STR_CONFIG_SETTING_HOVER_DELAY_HELPTEXT"
msgid "Delay before tooltips are displayed when hovering the mouse over some interface element. Alternatively tooltips are bound to the right mouse button when this value is set to 0"
msgstr "Retard abans que les descripcions emergents siguin mostrades quan el ratolí sigui sobre algun element de la interfície. Alternativament, les descripcions emergents poden ser vinculades al botó dret del ratolí si aquest paràmetre s'estableix en 0."

msgctxt "STR_CONFIG_SETTING_HOVER_DELAY_VALUE"
msgid "Hover for {COMMA} millisecond{P 0 \"\" s}"
msgstr "Manté durant {COMMA} milisegon{P 0 \"\" s}"

msgctxt "STR_CONFIG_SETTING_HOVER_DELAY_DISABLED"
msgid "Right click"
msgstr "Botó dret"

msgctxt "STR_CONFIG_SETTING_POPULATION_IN_LABEL"
msgid "Show town population in the town name label: {STRING2}"
msgstr "Mostra els habitants a l'etiqueta del nom de la població: {STRING}"

msgctxt "STR_CONFIG_SETTING_POPULATION_IN_LABEL_HELPTEXT"
msgid "Display the population of towns in their label on the map"
msgstr "Mostra els habitants a les etiquetes dels noms de les poblacions al mapa"

msgctxt "STR_CONFIG_SETTING_GRAPH_LINE_THICKNESS"
msgid "Thickness of lines in graphs: {STRING2}"
msgstr "Gruix de les línies dels gràfics: {STRING}"

msgctxt "STR_CONFIG_SETTING_GRAPH_LINE_THICKNESS_HELPTEXT"
msgid "Width of the line in the graphs. A thin line is more precisely readable, a thicker line is easier to see and colours are easier to distinguish"
msgstr "Gruix de la línia als gràfics. Una línia més fina és més llegible, una línia més gruixuda és més fàcil de veure i els colors són més fàcils de distingir"

msgctxt "STR_CONFIG_SETTING_SHOW_NEWGRF_NAME"
msgid "Show the NewGRF's name in the build vehicle window: {STRING2}"
msgstr "Mostra el nom del NewGRF a la finestra de construcció de vehicles: {STRING}"

msgctxt "STR_CONFIG_SETTING_SHOW_NEWGRF_NAME_HELPTEXT"
msgid "Add a line to the build vehicle window, showing which NewGRF the selected vehicle comes from"
msgstr "Afegeix una línia a la finestra de construcció de vehicles que mostri de quin NewGRF és el vehicle seleccionat."

msgctxt "STR_CONFIG_SETTING_SHOW_CARGO_IN_LISTS"
msgid "Show the cargoes the vehicles can carry in the list windows: {STRING2}"
msgstr "Mostra les càrregues que poden portar els vehicles a les finestres de llistes: {STRING}"

msgctxt "STR_CONFIG_SETTING_SHOW_CARGO_IN_LISTS_HELPTEXT"
msgid "If enabled, the vehicle's transportable load will appear above it in the vehicle lists"
msgstr "Si s'activa, la càrrega transportable del vehicle apareixerà al damunt de les llistes dels vehicles."

msgctxt "STR_CONFIG_SETTING_LANDSCAPE"
msgid "Landscape: {STRING2}"
msgstr "Paisatge: {STRING}"

msgctxt "STR_CONFIG_SETTING_LANDSCAPE_HELPTEXT"
msgid "Landscapes define basic gameplay scenarios with different cargoes and town growth requirements. NewGRF and Game Scripts allow finer control though"
msgstr "El tipus de paisatge defineix escenaris amb diferents tipus de càrrega i requisits per al creixement de les poblacions. Els NewGRF i l'script de partida poden modificar-ne l'aparença i el comportament."

msgctxt "STR_CONFIG_SETTING_LAND_GENERATOR"
msgid "Land generator: {STRING2}"
msgstr "Generador de terrenys: {STRING}"

msgctxt "STR_CONFIG_SETTING_LAND_GENERATOR_HELPTEXT"
msgid "The original generator depends on the base graphics set, and composes fixed landscape shapes. TerraGenesis is a Perlin noise based generator with finer control settings"
msgstr "L'algorisme generador original depèn del tipus de gràfics base i crea formes de terreny predefinides. TerraGenesis és un generador basat en el soroll Perlin que permet una configuració més precisa"

msgctxt "STR_CONFIG_SETTING_TERRAIN_TYPE"
msgid "Maximum height preset: {STRING2}"
msgstr "Perfil d'altitud màxima: {STRING}"

msgctxt "STR_CONFIG_SETTING_TERRAIN_TYPE_HELPTEXT"
msgid "Choose the height of hills and mountains of the landscape"
msgstr "Escolliu l'altura dels turons i muntanyes del paisatge."

msgctxt "STR_CONFIG_SETTING_INDUSTRY_DENSITY"
msgid "Industry density: {STRING2}"
msgstr "Densitat d'indústries: {STRING}"

msgctxt "STR_CONFIG_SETTING_INDUSTRY_DENSITY_HELPTEXT"
msgid "Set how many industries should be generated and what level should be maintained during the game"
msgstr "Fixa el nombre d'indústries que s'haurien de generar i mantenir durant la partida"

msgctxt "STR_CONFIG_SETTING_OIL_REF_EDGE_DISTANCE"
msgid "Maximum distance from edge for Oil industries: {STRING2}"
msgstr "Distància màxima des de la vora a les indústries de petroli: {STRING}"

msgctxt "STR_CONFIG_SETTING_OIL_REF_EDGE_DISTANCE_HELPTEXT"
msgid "Limit for how far from the map border oil refineries and oil rigs can be constructed. On island maps this ensures they are near the coast. On maps larger than 256 tiles, this value is scaled up"
msgstr "Limita a quina distància del mapa es poden construir refineries de petroli i plataformes petrolíferes. En mapes amb illes, serveix per assegurar que estan prop de la costa. En mapes de més de 256 caselles, aquest valor s'escala automàticament."

msgctxt "STR_CONFIG_SETTING_SNOWLINE_HEIGHT"
msgid "Snow line height: {STRING2}"
msgstr "Alçada dels estatges amb neu: {STRING}"

msgctxt "STR_CONFIG_SETTING_SNOWLINE_HEIGHT_HELPTEXT"
msgid "Choose at what height snow starts in sub-arctic landscape. Snow also affects industry generation and town growth requirements. Can only be modified via Scenario Editor or is otherwise calculated via \"snow coverage\""
msgstr "Tria a quina altura comença a aparèixer neu al paisatge subàrtic. La neu també afecta a la generació d'indústries i els requisits per al creixement de les poblacions. Només es pot modificar a l'editor d'escenaris; altrament, es calcula fent servir la proporció de terreny nevat."

msgctxt "STR_CONFIG_SETTING_SNOW_COVERAGE"
msgid "Snow coverage: {STRING2}"
msgstr "Terreny nevat: {STRING}"

msgctxt "STR_CONFIG_SETTING_SNOW_COVERAGE_HELPTEXT"
msgid "Choose the approximate amount of snow on the sub-arctic landscape. Snow also affects industry generation and town growth requirements. Only used during map generation. Sea level and coast tiles never have snow"
msgstr "Escolliu la quantitat aproximada de neu als paisatges subàrtics. La neu també afecta a la generació d'indústries i els requisits per al creixement de les poblacions. Només es fa servir durant la generació de mapes. A nivell de mar i a les caselles costeres mai hi haurà neu."

msgctxt "STR_CONFIG_SETTING_SNOW_COVERAGE_VALUE"
msgid "{NUM}%"
msgstr "{NUM}{NBSP}%"

msgctxt "STR_CONFIG_SETTING_DESERT_COVERAGE"
msgid "Desert coverage: {STRING2}"
msgstr "Terreny desèrtic: {STRING}"

msgctxt "STR_CONFIG_SETTING_DESERT_COVERAGE_HELPTEXT"
msgid "Choose the approximate amount of desert on the tropical landscape. Desert also affects industry generation and town growth requirements. Only used during map generation"
msgstr "Escolliu la quantitat aproximada de desert dels paisatges tropicals. El desert afecta a la generació d'indústries i els requisits de creixement de les poblacions. Només es fa servir durant la generació de mapes."

msgctxt "STR_CONFIG_SETTING_DESERT_COVERAGE_VALUE"
msgid "{NUM}%"
msgstr "{NUM}{NBSP}%"

msgctxt "STR_CONFIG_SETTING_ROUGHNESS_OF_TERRAIN"
msgid "Roughness of terrain: {STRING2}"
msgstr "Geografia del terreny: {STRING}"

msgctxt "STR_CONFIG_SETTING_ROUGHNESS_OF_TERRAIN_HELPTEXT"
msgid "Choose the shape and number of hills. Smooth landscapes have fewer, wider hills, while rough landscapes have more, smaller hills"
msgstr "Trieu la forma i quantitat de turons. Els terrenys més suaus tenen menys turons i són més amples, mentre que els terrenys més irregulars tenen més turons i són més petits."

msgctxt "STR_CONFIG_SETTING_ROUGHNESS_OF_TERRAIN_VERY_SMOOTH"
msgid "Very Smooth"
msgstr "Molt suau"

