msgstr "Desactivat"

msgctxt "STR_CONFIG_SETTING_NEWS_MESSAGES_SUMMARY"
msgid "Summary"
msgstr "Resum"

msgctxt "STR_CONFIG_SETTING_NEWS_MESSAGES_FULL"
